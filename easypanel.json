{
  "name": "myagent-dash",
  "description": "AI Platform v1 - Digital Workers Manager",
  "version": "1.0.0",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "environment": {
    "NODE_ENV": "production",
    "VITE_SUPABASE_URL": "https://your-project.supabase.co",
    "VITE_SUPABASE_ANON_KEY": "your-anon-key"
  },
  "secrets": [
    "SUPABASE_URL",
    "SUPABASE_ANON_KEY",
    "SUPABASE_SERVICE_ROLE_KEY",
    "STRIPE_SECRET_KEY",
    "STRIPE_PUBLISHABLE_KEY",
    "SENDGRID_API_KEY"
  ],
  "ports": [
    {
      "port": 3000,
      "protocol": "http"
    }
  ],
  "healthcheck": {
    "path": "/",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  }
}